<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cl.vicentepc.miappprueba3">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="FavoritePlaces"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".login.LoginActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="FavoritePlaces"
            android:theme="@style/FullscreenTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".views.main.MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".views.results.ResultActivity" android:parentActivityName=".views.main.MainActivity"/>
        <activity android:name=".views.results.PlaceInfoActivity" android:parentActivityName=".views.main.MainActivity"/>
    </application>

</manifest>